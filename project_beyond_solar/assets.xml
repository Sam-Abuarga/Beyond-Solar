<odoo>
    <template id="assets_backend" name="Beyond Solar Project Backend Assets" inherit_id="web.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/project_beyond_solar/static/src/scss/map_view.scss"/>
        </xpath>

        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/project_beyond_solar/static/src/js/calendar_renderer.js"></script>
            <script type="text/javascript" src="/project_beyond_solar/static/src/js/map_controller.js"></script>
            <script type="text/javascript" src="/project_beyond_solar/static/src/js/map_renderer.js"></script>
        </xpath>
    </template>

    <template id="assets_frontend" name="Beyond Solar Project Frontend Assets" inherit_id="web.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/project_beyond_solar/static/src/scss/project.scss"/>
        </xpath>
    </template>
</odoo>
